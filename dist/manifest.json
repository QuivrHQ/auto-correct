{
  "manifest_version": 3,
  "name": "AutoCorrect by Quivr",
  "description": "Correction grammaticale et orthographique en temps r√©el",
  "version": "1.2.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-DhH41zZQ.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/storage-BoEp-vZp.js",
        "assets/language-tool-client-kH6335wn.js",
        "assets/index.ts-CcbaLece.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
